<h1>Listing Songs</h1>

<table>
	<thead>
		<tr>
			<th>
			</th>
			<th>
				Name
			</th>
			<th>
				Artist(s)
			</th>
		</tr>
	</thead>
	<tbody>
		<% @songs.each do |song| %>
			<tr>
				<td>
					<%= song.id %>
				</td>
				<td>
					<%= song.name %>
				</td>
				<td>
					<%= song.artists.map(&:name).join(', ') %>
				</td>
			</tr>
		<% end %>
	</tbody>
</table>
